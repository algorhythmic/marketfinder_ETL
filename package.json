{
  "name": "flex-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm-run-all --parallel dev:frontend dev:backend",
    "dev:frontend": "vite --open",
    "dev:backend": "convex dev",
    "lint": "tsc -p convex -noEmit --pretty false && tsc -p . -noEmit --pretty false && convex dev --once && vite build",
    "test:etl": "tsx api/test-etl/test-final-pipeline.ts",
    "build": "vite build",
    "typecheck": "tsc --noEmit",
    "setup:duckdb": "tsx scripts/setup-local-duckdb.ts",
    "test:pagination": "tsx scripts/test-pagination-with-duckdb.ts",
    "query:duckdb": "tsx scripts/query-duckdb.ts",
    "pagination:summary": "tsx scripts/pagination-summary.ts",
    "duckdb:server": "tsx scripts/duckdb-server.ts",
    "populate:full": "tsx scripts/populate-full-database.ts",
    "populate:exhaustive": "tsx scripts/populate-exhaustive-database.ts",
    "populate:comprehensive": "POLYMARKET_BATCHES=100 tsx scripts/populate-exhaustive-database.ts",
    "populate:complete": "POLYMARKET_BATCHES=520 tsx scripts/populate-exhaustive-database.ts"
  },
  "dependencies": {
    "@convex-dev/auth": "^0.0.80",
    "@duckdb/node-api": "^1.3.1-alpha.22",
    "@duckdb/node-bindings-win32-x64": "^1.3.1-alpha.22",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@tanstack/react-table": "^8.21.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "convex": "^1.24.2",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "duckdb": "^1.1.3",
    "express": "^5.1.0",
    "lucide-react": "^0.511.0",
    "node-fetch": "^3.3.2",
    "openai": "^4.75.0",
    "react": "^18.2.0",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.2.0",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.3.0",
    "usehooks-ts": "^3.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^22.13.10",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "~10",
    "dotenv": "^16.4.7",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "npm-run-all": "^4.1.5",
    "postcss": "~8",
    "prettier": "^3.5.3",
    "tailwindcss": "~3",
    "tsx": "^4.20.3",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0"
  }
}
